source $HOME/.profile

# Shell stuff
alias ls='ls --color=tty'
alias ll='ls -la'
alias grep='grep  --color=auto'
alias clipboard="xclip -sel clip <"
alias zshreload="source $HOME/.zshrc"
alias polybarreload="$HOME/.config/polybar/launch.sh"

#Python

#GO
export GOPATH=$HOME/.local/go
export GO111MODULE=on
export PATH=$PATH:$GOPATH/bin

#can't install package with [] in the name because of zsh file globbing
alias pip='noglob pip'
alias pip3='noblob pip3'

#Emacs
alias e=emacs
alias es="emacs -nw"
alias ed="emacs --daemon"
alias ec="emacsclient -t"

#TMUX
alias  devmux="~/.config/tmux/devmux.sh"
alias dual_monitor='nvidia-settings --assign "CurrentMetaMode=DPY-5: 2560x1440_144 @2560x1440 +0+0 {ViewPortIn=2560x1440, ViewPortOut=2560x1440+0+0, ForceCompositionPipeline=On, ForceFullCompositionPipeline=On}, DPY-3: 2560x1440_144 @2560x1440 +2560+0
{ViewPortIn=2560x1440, ViewPortOut=2560x1440+0+0, ForceCompositionPipeline=On, ForceFullCompositionPipeline=On}"'
